{
  "name": "spsc",
  "version": "0.0.1",
  "description": "spsc testbed",
  "author": "qbane",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:lib": "svelte-package -i src/lib/spsc",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || true",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.app.json",
    "tsc": "tsc -b jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": ["dist/"],
  "main": "./dist/common.js",
  "types": "./dist/common.d.ts",
  "exports": {
    ".": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js"
    },
    "./reader": {
      "types": "./dist/reader.d.ts",
      "import": "./dist/reader.js"
    },
    "./writer": {
      "types": "./dist/writer.d.ts",
      "import": "./dist/writer.js"
    }
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "~5.2.13",
    "@sveltejs/kit": "~2.27.0",
    "@sveltejs/package": "^2.4.1",
    "@sveltejs/vite-plugin-svelte": "~5.1.1",
    "@types/node": "~22.17.0",
    "svelte": "~5.37.3",
    "svelte-check": "~4.3.1",
    "typescript": "~5.9.2",
    "vite": "~6.3.5",
    "vitest": "~3.2.4"
  },
  "engines": {
    "node": ">=20.0.0 <25.0.0"
  }
}
