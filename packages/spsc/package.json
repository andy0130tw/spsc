{
  "name": "spsc",
  "version": "0.0.2",
  "description": "Single-producer-single-consumer queue that adheres to POSIX's pipe semantics.",
  "author": "qbane",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/andy0130tw/spsc",
    "directory": "packages/spsc"
  },
  "scripts": {
    "dev": "vite build -w",
    "build": "vite build",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist/"
  ],
  "main": "./dist/common.js",
  "types": "./dist/common.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/common.d.cts",
        "default": "./dist/common.cjs"
      },
      "import": {
        "types": "./dist/common.d.ts",
        "default": "./dist/common.js"
      }
    },
    "./reader": {
      "require": {
        "types": "./dist/reader.d.cts",
        "default": "./dist/reader.cjs"
      },
      "import": {
        "types": "./dist/reader.d.ts",
        "default": "./dist/reader.js"
      }
    },
    "./writer": {
      "require": {
        "types": "./dist/writer.d.cts",
        "default": "./dist/writer.cjs"
      },
      "import": {
        "types": "./dist/writer.d.ts",
        "default": "./dist/writer.js"
      }
    }
  },
  "devDependencies": {
    "@types/node": "~22.17.0",
    "typescript": "~5.9.2",
    "vite": "~6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "~3.2.4"
  },
  "engines": {
    "node": ">=20.0.0 <25.0.0"
  }
}
