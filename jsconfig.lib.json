{
  "extends": "./jsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "nodenext",
    "sourceMap": true,
    "tsBuildInfoFile": "node_modules/.tmp/lib.tsbuildinfo"
  },
  "include": [
    "./src/lib/spsc/**/*.js",
    "./src/lib/spsc/**/*.ts"
	],
